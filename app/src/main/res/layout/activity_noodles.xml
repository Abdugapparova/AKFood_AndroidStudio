<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#E9F1E3"
    tools:context=".typeOfFoods.Noodles">

    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginStart="30dp"
    android:layout_marginEnd="30dp"
    android:orientation="vertical"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/noodlesO"
            android:layout_width="match_parent"
            android:layout_height="57dp"
            android:layout_marginTop="40dp"
            android:fontFamily="sans-serif-smallcaps"
            android:text="Noodles"
            android:textAlignment="center"
            android:textColor="#8AAC74"
            android:textSize="34sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="159dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/korean"
                android:layout_width="184dp"
                android:layout_height="156dp"
                app:srcCompat="@drawable/korean" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/cBurger"
                    android:layout_width="153dp"
                    android:layout_height="55dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="30dp"
                    android:fontFamily="sans-serif-smallcaps"
                    android:text="Spicy Korean Beef Noodle"
                    android:textAlignment="center"
                    android:textColor="#0B0A0A"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/textView36"
                    android:layout_width="153dp"
                    android:layout_height="38dp"
                    android:layout_marginStart="10dp"
                    android:fontFamily="sans-serif-smallcaps"
                    android:text="3200  TG"
                    android:textAlignment="center"
                    android:textColor="#0B0A0A"
                    android:textSize="20sp" />
            </LinearLayout>
        </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="169dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/rosa"
            android:layout_width="184dp"
            android:layout_height="156dp"
            android:layout_marginTop="10dp"
            app:srcCompat="@drawable/rosa" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/a"
                android:layout_width="153dp"
                android:layout_height="84dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="30dp"
                android:fontFamily="sans-serif-smallcaps"
                android:text="Cheese Tortelloni Rosa"
                android:textAlignment="center"
                android:textColor="#0B0A0A"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView32"
                android:layout_width="153dp"
                android:layout_height="38dp"
                android:layout_marginStart="10dp"
                android:fontFamily="sans-serif-smallcaps"
                android:text="2300 TG"
                android:textAlignment="center"
                android:textColor="#0B0A0A"
                android:textSize="20sp" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/tofu"
            android:layout_width="184dp"
            android:layout_height="156dp"
            android:layout_marginTop="10dp"
            app:srcCompat="@drawable/tofu" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/beefB"
                android:layout_width="153dp"
                android:layout_height="84dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="30dp"
                android:fontFamily="sans-serif-smallcaps"
                android:text="Japanese Pan Noodles with Tofu"
                android:textAlignment="center"
                android:textColor="#0B0A0A"
                android:textSize="20sp" />

            <TextView
                android:id="@+id/textView34"
                android:layout_width="153dp"
                android:layout_height="38dp"
                android:layout_marginStart="10dp"
                android:fontFamily="sans-serif-smallcaps"
                android:text="4500 TG"
                android:textAlignment="center"
                android:textColor="#0B0A0A"
                android:textSize="20sp" />

        </LinearLayout>

    </LinearLayout>

</LinearLayout>

</android.support.constraint.ConstraintLayout>
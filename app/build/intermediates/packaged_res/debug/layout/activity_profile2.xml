<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile2">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F4EBE8"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/profilePage2"
            android:layout_width="163dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="120dp"
            android:layout_marginTop="50dp"
            android:layout_marginBottom="20dp"
            android:drawableLeft="@drawable/ic_baseline_person_35"
            android:fontFamily="sans-serif-smallcaps"
            android:text="Profile"
            android:textAlignment="center"
            android:textColor="#F6AE97"
            android:textSize="34sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="688dp"
            android:layout_marginLeft="50dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="301dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="33dp"
                    android:layout_height="wrap_content"
                    android:drawableLeft="@drawable/ic_baseline_person_35_black"
                    android:fontFamily="sans-serif-smallcaps"
                    android:textColor="#303030"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="259dp"
                    android:layout_height="33dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginEnd="139dp"
                    android:layout_marginRight="139dp"
                    android:fontFamily="sans-serif-smallcaps"
                    android:singleLine="true"
                    android:textColor="#303030"
                    android:textSize="20sp"
                    android:textStyle="normal"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/imageView2"
                    app:layout_constraintTop_toTopOf="parent" />
            </LinearLayout>

            <TableRow
                android:id="@+id/hr"
                android:layout_width="301dp"
                android:layout_height="1dp"
                android:background="#444"></TableRow>

            <LinearLayout
                android:layout_width="301dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="wrap_content"
                    android:layout_height="33dp"
                    android:drawableLeft="@drawable/ic_baseline_email_35"
                    android:fontFamily="sans-serif-smallcaps"
                    android:textColor="#303030"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/textView13"
                    android:layout_width="259dp"
                    android:layout_height="33dp"
                    android:layout_marginLeft="10dp"
                    android:fontFamily="sans-serif-smallcaps"
                    android:textColor="#303030"
                    android:textSize="20sp"
                    android:textStyle="normal"
                    app:layout_constraintStart_toEndOf="@+id/textView6" />
            </LinearLayout>
            <TableRow
                android:id="@+id/hr1"
                android:layout_width="301dp"
                android:layout_height="1dp"
                android:background="#444"></TableRow>

            <LinearLayout
                android:layout_width="301dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView21"
                    android:layout_width="wrap_content"
                    android:layout_height="33dp"
                    android:drawableLeft="@drawable/ic_baseline_local_phone_24"
                    android:fontFamily="sans-serif-smallcaps"
                    android:textColor="#303030"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="260dp"
                    android:layout_height="33dp"
                    android:layout_marginLeft="10dp"
                    android:fontFamily="sans-serif-smallcaps"
                    android:textColor="#303030"
                    android:textSize="20sp"
                    android:textStyle="normal"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView13" />
            </LinearLayout>
            <TableRow
                android:id="@+id/hr2"
                android:layout_width="301dp"
                android:layout_height="1dp"
                android:background="#444"></TableRow>

            <LinearLayout
                android:layout_width="301dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:orientation="horizontal"
                android:layout_marginBottom="10dp">

                <TextView
                    android:id="@+id/textView14"
                    android:layout_width="wrap_content"
                    android:layout_height="33dp"
                    android:drawableLeft="@drawable/ic_baseline_lock_35"
                    android:fontFamily="sans-serif-smallcaps"
                    android:textColor="#303030"
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="254dp"
                    android:layout_height="33dp"
                    android:layout_marginLeft="10dp"
                    android:fontFamily="sans-serif-smallcaps"
                    android:textColor="#303030"
                    android:textSize="20sp"
                    android:textStyle="normal"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView13" />
            </LinearLayout>

            <TableRow
                android:id="@+id/hr4"
                android:layout_width="301dp"
                android:layout_height="1dp"
                android:background="#444">

            </TableRow>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <Button
                    android:id="@+id/logout"
                    android:layout_width="301dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="300dp"
                    android:background="@drawable/my_button"
                    android:text="Log out"
                    android:textColor="#FDFCFC"
                    android:textSize="20sp" />
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</android.support.constraint.ConstraintLayout>